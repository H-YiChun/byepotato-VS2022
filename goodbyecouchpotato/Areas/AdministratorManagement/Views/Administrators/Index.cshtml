@model List<AdminViewModel>
@{
    ViewData["Title"] = "Index";
}
@section Styles
{
    <style>
    </style>
}

<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Roles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>
                    @foreach (var role in user.Roles)
                    {
                        <span>@role.RoleName</span>

                        <br />
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
